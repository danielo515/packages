{
  "name": "sanfu",
  "version": "2.0.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "pretest": "npm run build",
    "prepublish": "rm -rf lib && npm run build",
    "postpublish": "npm run docs && npm run docs:publish",
    "build": "NODE_ENV=production babel ./src --extensions '.ls' -d lib",
    "build:test": "lsc -b -c test",
    "test": "lab --reporter console -o stdout --threshold 50 --assert code -P spec -f -T test/transformer --verbose test",
    "docs": "jsdoc src -d docs -R README.md -c jsdoc.json",
    "docs:publish": "gh-pages -p docs -r  https://${GH_TOKEN}@github.com/danielo515/sanfu -b master -m 'chore: automatic docs build'",
    "watch": "babel -w ./src --extensions '.ls' -d lib",
    "start": "lsc execute"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "sanctuary": "^0.15.0"
  },
  "repository": {
    "url": "https://github.com/danielo515/packages.git",
    "type": "git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-all-curried": "^4.0.0",
    "babel-plugin-curry-all": "^2.0.2",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "code": "^5.2.0",
    "gh-pages": "^1.2.0",
    "jsdoc": "^3.5.5",
    "lab": "^15.5.0",
    "livescript": "^1.5.0",
    "livescript-next": "^0.0.4"
  }
}
